{
    "name": "halloverden/symfony-validator-constraints-bundle",
    "description": "Various constraints for symfony validator",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Tom Roar Furunes",
            "email": "tom.roar@halloverden.no"
        },
        {
            "name": "Erich Json Fosse",
            "email": "erich@halloverden.no"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": ">=8.1",
        "symfony/http-kernel": "^5.4|^6.1",
        "symfony/dependency-injection": "^5.4|^6.1",
        "symfony/validator": "^5.4|^6.1",
        "symfony/framework-bundle": "^5.4|^6.1",
        "doctrine/annotations": "^1.8",
        "symfony/property-access": "^5.4|^6.1",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "HalloVerden\\ValidatorConstraintsBundle\\": ""
        }
    },
    "require-dev": {
        "doctrine/doctrine-bundle": "^2.0",
        "giggsey/libphonenumber-for-php": "^8.11",
        "jms/serializer": "^3.4",
        "kickbox/kickbox": "^2.2",
        "league/uri": "^6.2"
    }
}
